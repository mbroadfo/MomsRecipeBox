{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "cross-env VITE_ENVIRONMENT=local vite --mode development",
    "dev:atlas": "cross-env VITE_ENVIRONMENT=atlas vite --mode development",
    "dev:lambda": "cross-env VITE_ENVIRONMENT=lambda vite --mode development",
    "build": "tsc -b && vite build --mode production",
    "build:local": "cross-env VITE_ENVIRONMENT=local tsc -b && vite build --mode development",
    "build:atlas": "cross-env VITE_ENVIRONMENT=atlas tsc -b && vite build --mode development",
    "build:lambda": "cross-env VITE_ENVIRONMENT=lambda tsc -b && vite build --mode production",
    "build:production": "cross-env VITE_ENVIRONMENT=production tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:local": "npm run build:local && vite preview",
    "preview:atlas": "npm run build:atlas && vite preview",
    "preview:lambda": "npm run build:lambda && vite preview"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.8.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.0.0"
  }
}
